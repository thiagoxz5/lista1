#include <stdio.h>

int main(void) {
  float vel;
  printf("Digite a velocidade do carro: \n");
  scanf("%f", &vel);
  if (vel > 50 && vel <= 60){
    printf("O valor a ser pago é de 50 reais.");
  }
  if (vel > 60 && vel <= 80){
    printf("O valor a ser pago é de 100 reais.");
  }
  if (vel > 80){
    printf("O valor a ser pago é de 200 reais.");
  }
  if (vel <= 50){
    printf("Não há multa a se pagar.");
  }
}
