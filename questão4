#include <stdio.h>

int main(void) {
  float nota; //define a variavel
  printf("Digite sua nota: \n"); //escreve na tela para o usuario digitar sua nota
  scanf("%f", &nota);
  if (nota < 5) { // usei uma estrutura de condição, onde so se escreve na tela o valor que se encaixa na devida condição
    printf("Insatisfatorio");
  } else if (nota >= 5 && nota < 7) {
    printf("Regular");
  } else if (nota >= 7 && nota < 8) {
    printf("Bom");
  } else if (nota >= 8 && nota <= 10) {
    printf("Otimo");
  }
}
